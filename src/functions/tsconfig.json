{
  "compileOnSave": true,
  "compilerOptions": {
    "lib": ["es6"],
    "module": "commonjs",
    "outDir": "../../dist/functions",
    "allowJs": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "skipLibCheck": true,
    "noEmitOnError": true,
    "target": "es6"
  }
}
