{
  "name": "oyah-nextjs",
  "version": "1.0.0",
  "description": "Oyah's website source code",
  "scripts": {
    "old-dev": "nodemon server/index.ts",
    "dev": "concurrently \"next\" \"firebase functions:shell\"",
    "preserve": "npm run clean && npm run build-all && npm run copy-deps && npm run install-deps",
    "serve": "NODE_ENV=production firebase serve --only functions,hosting",
    "deploy-all": "firebase deploy",
    "clean": "rimraf \"dist/functions\" && rimraf \"dist/public\"",
    "build-public": "cpx \"public/**/*.*\" \"dist/public\" -C",
    "build-app": "NODE_ENV=production next build",
    "build-funcs": "tsc --project ./functions/tsconfig.json",
    "build-all": "npm run build-public && npm run build-funcs && npm run build-app",
    "copy-deps": "cpx \"./*{package.json,package-lock.json}\" \"dist/functions\"",
    "install-deps": "cd \"dist/functions\" && npm i",
    "build": "next build && tsc --project tsconfig.server.json",
    "start": "NODE_ENV=production node production-server/index.js",
    "analyze": "cross-env ANALYZE=1 next build"
  },
